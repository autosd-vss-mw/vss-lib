# Global configuration section
[global]
# Timer interval to re-print messages in the D-Bus interface (in seconds or microseconds)
timer=2  # Please use: seconds or microseconds (specify explicitly)

# Path for all VSS files, defined as a macro for easy reuse
vspec_path=/usr/share/vss-lib/

# Toyota configuration
[vehicle_toyota]
vendor=toyota
vspec_file=/usr/share/vss-lib/toyota.vspec
preference=ASIL  # Can be QM, ASIL or QM, ASIL or UserPreference
attach_electronics=bosch,renesas  # Electronics vendors to attach

# BMW configuration
[vehicle_bmw]
vendor=bmw
vspec_file=/usr/share/vss-lib/bmw.vspec
preference=QM
attach_electronics=bosch  # Electronics vendors to attach
